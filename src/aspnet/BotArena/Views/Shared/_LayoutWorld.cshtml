@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <script type="text/javascript">
        var gosuArena = gosuArena || {};

        gosuArena.versionNumber = '@GosuArena.Infrastructure.Environment.VersionIdentifier';

        gosuArena.url = gosuArena.url || {};
        gosuArena.url.baseUrl = '@Url.Content("~/")';
        gosuArena.url.cacheBustingFragment = gosuArena.versionNumber;
    </script>

    <link rel="stylesheet" href="@Url.Content("~/Content/Pages/Match/World.less")" />

    @RenderSection("styles", required: false)

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/game")
    @Scripts.Render("~/bundles/menu")
    <script type="text/javascript" src='@Url.Content("~/signalr/hubs")'></script>

    <link rel="stylesheet" href="@Url.Content("~/Content/Menu.less")" />
    <link rel="stylesheet" href="@Url.Content("~/Content/lib/ng-toaster/toaster.min.css")" />
</head>
<body>
    @*<button id="main-menu-btn"class="btn">Menu</button>*@
    @RenderBody()

    @Html.Partial("Menu")

<aside id="notificationContainer" class="notifications"></aside>

<script type="text/javascript" src="@Url.Content("~/Scripts/lib/underscore-min.js")"> </script>
@Scripts.Render("~/bundles/jqueryvalidation")
<script type="text/javascript" src="@Url.Content("~/Content/lib/bootstrap-3.0.3/js/alert.js")"> </script>
<script type="text/javascript" src="@Url.Content("~/Content/lib/bootstrap-notify/js/bootstrap-notify.js")"> </script>
<script type="text/javascript" src="@Url.Content("~/Scripts/Pages/confirmation.js")"> </script>
<script type="text/javascript" src="@Url.Content("~/Scripts/Pages/notifications.js")"> </script>
<script type="text/javascript" src="@Url.Content("~/Scripts/Pages/changetracking.js")"> </script>

@RenderSection("scripts", required: false)


<script type="text/javascript">
    $(document).ready(function () {
        var menu = gosuArena.factories.createMenu();
        menu.initialize();
    });
</script>

</body>
</html>